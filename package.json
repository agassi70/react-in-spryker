  "scripts": {
    "cpy:local:inside": "cpy local_inside_vm.env ./ --rename=.env",
    "cpy:local:outside": "cpy local_outside_vm.env ./ --rename=.env",
    "cpy:local:docker": "cpy local_docker.env ./ --rename=.env",
    "cpy:prod": "cpy prod.env ./ --rename=.env",
    "build:web": "node --max_old_space_size=6144 node_modules/.bin/webpack-cli --config ./webpack.config.js --progress --profile --colors",
    "build:web:prod": "cross-env NODE_ENV=production npm run build:web",
    "dist": "npm run build:web:prod",
    "dist:dev": "npm run cpy:local:outside && npm run build:web",
    "start:docker": "npm run cpy:local:docker && docker-compose up --build",
    "dist:prod": "npm run cpy:prod && npm run build:web:prod",
    "dev": "cross-env NODE_ENV=webpack-dev-server node --max_old_space_size=4096 server.js",
    "serve:vm": "npm run cpy:local:inside && npm run dev",
    "serve:local": "npm run cpy:local:outside && npm run dev",
    "start": "node ./server.js",
